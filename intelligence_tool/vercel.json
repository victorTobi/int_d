{
    "version": 2,
    "builds": [
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "src/js/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/api/eth",
        "dest": "/src/js/eth.js"
      },
      {
        "src": "/api/tron",
        "dest": "/src/js/tron.js"
      },
      {
        "src": "/api/bitcoin",
        "dest": "/src/js/bitcoin.js"
      },
      {
        "src": "/api/dogecoin",
        "dest": "/src/js/dogecoin.js"
      },
      {
        "src": "/api/solana",
        "dest": "/src/js/solana.js"
      },
      {
        "src": "/api/bnb",
        "dest": "/src/js/bnb.js"
      },
      {
        "src": "/api/telegram",
        "dest": "/src/js/telegram.js"
      }
    ]
  }
  